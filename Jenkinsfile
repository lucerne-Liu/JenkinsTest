#!/usr/bin/env groovy Jenkinsfile
pipeline {
    agent any

    stages {
        stage('Declarative:Checkout SCM') {
                steps {
                    echo 'Declarative:Checkout SCM'
                }
            }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }

    }
}
